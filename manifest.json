{
  "manifest_version": 3,
  "name": "Prompts Manager",
  "description": "Save, organize, and manage your writing prompts.",
  "version": "1.0",
  "permissions": ["activeTab", "storage"],
  "action": {
    "default_popup": "popup.html",
    "default_title": "Prompt Manager"
  },
  "icons": {
    "16": "logo_16.png",
    "24": "logo_24.png",
    "32": "logo_32.png",
    "48": "logo_48.png",
    "128": "logo_128.png"
  }
}
